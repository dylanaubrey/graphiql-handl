{
  "name": "graphiql-handl",
  "version": "0.0.1",
  "description": "An in-browser IDE for exploring GraphQL with Handl.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/graphiql-handl",
  "bugs": {
    "url": "https://github.com/dylanaubrey/graphiql-handl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/graphiql-handl"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "types": "lib/main/index.d.ts",
  "keywords": [
    "handl",
    "graphql",
    "graphiql"
  ],
  "bin": {
    "graphiql-handl": "bin/graphiql-handl.js"
  },
  "scripts": {
    "analyze:bundle": "webpack-bundle-analyzer bundle/stats.json lib/umd --mode static --report bundle/report.html --no-open",
    "build": "npm run clean && npm run test && npm run compile && npm run analyze:bundle && npm run document",
    "clean": "gulp clean",
    "compile": "npm run compile:main && npm run compile:module && npm run compile:browser && npm run compile:umd",
    "compile:browser": "cross-env BABEL_ENV=browser gulp browser",
    "compile:main": "cross-env BABEL_ENV=main gulp main",
    "compile:module": "cross-env BABEL_ENV=module gulp module",
    "compile:umd": "cross-env BABEL_ENV=browser gulp umd",
    "debug:web": "cross-env BABEL_ENV=browser TEST_ENV=true karma start --no-single-run --reporters progress --browsers",
    "graphiql-handl": "node bin/graphiql-handl",
    "lint": "gulp lint",
    "publish:pkg": "npm run build && npm publish",
    "test": "npm run lint && npm run type-check && npm run test:web && npm run test:node",
    "test:node": "cross-env TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "test:web": "cross-env BABEL_ENV=browser TEST_ENV=true karma start --single-run --browsers ChromeHeadless --reporters mocha,coverage-istanbul",
    "type-check": "gulp type-check"
  },
  "dependencies": {
    "graphiql": "^0.11.11",
    "handl": "^0.1.8",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.10.5 || ^0.11.3 || ^0.12.0 || ^0.13.0",
    "prop-types": ">=15.5.0",
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.43",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.43",
    "@babel/preset-env": "^7.0.0-beta.43",
    "@babel/preset-react": "^7.0.0-beta.43",
    "@babel/preset-stage-0": "^7.0.0-beta.43",
    "@types/chai": "^4.0.8",
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.12.7",
    "@types/lodash": "^4.14.85",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.1",
    "@types/react-dom": "^16.0.4",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "@types/yargs": "^11.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-eslint": "^8.0.2",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.0.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^4.0.2",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "lodash-webpack-plugin": "^0.11.4",
    "merge-stream": "^1.0.1",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-stream": "^4.0.0"
  }
}
